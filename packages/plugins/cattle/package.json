{
  "name": "@clawlets/plugin-cattle",
  "version": "0.4.3",
  "private": false,
  "type": "module",
  "description": "Clawlets cattle plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/regenrek/clawlets"
  },
  "exports": {
    ".": "./dist/plugin.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clawlets/cattle-core": "workspace:*",
    "@clawlets/clf-queue": "workspace:*",
    "@clawlets/core": "workspace:*",
    "@clawlets/shared": "workspace:*",
    "better-sqlite3": "^12.6.2",
    "citty": "^0.2.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
"@types/node": "^25.1.0",

    "@vitest/coverage-v8": "^4.0.18",
    "tsdown": "^0.20.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22"
  },
  "clawlets": {
    "command": "cattle",
    "entry": "./dist/plugin.mjs"
  },
  "packageManager": "pnpm@10.25.0"
