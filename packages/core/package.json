{
  "name": "@clawdlets/core",
  "version": "0.4.3",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./doctor": "./dist/doctor.js",
    "./repo-layout": "./dist/repo-layout.js",
    "./lib/*": "./dist/lib/*.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && node ../../scripts/copy-core-assets.mjs",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "pretest": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "test": "vitest",
    "pretest:run": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "test:run": "vitest run",
    "precoverage": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clawdlets/cattle-core": "workspace:*",
    "@clawdlets/shared": "workspace:*",
    "ajv": "^8.17.1",
    "dotenv": "^17.2.3",
    "giget": "^3.1.1",
    "zod": "^4.3.6",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.25.0"
}
