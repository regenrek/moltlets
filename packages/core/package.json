{
  "name": "@clawdlets/core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./doctor": "./dist/doctor.js",
    "./repo-layout": "./dist/repo-layout.js",
    "./lib/*": "./dist/lib/*.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "pretest": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "test": "vitest",
    "pretest:run": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "test:run": "vitest run",
    "precoverage": "pnpm -C . build && node ../../scripts/fetch-template-for-tests.mjs",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "zod": "^3.25.76",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.13",
    "typescript": "^5.6.3",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.25.0"
}
